    "rimraf": "^3.0.2",
    "stylelint": "11.1.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "textlint": "^11.7.6",
    "textlint-rule-no-todo": "^2.0.1"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:js",
      "npm run textlint:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
