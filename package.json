    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "22.19.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-watch": "6.0.1",
    "flow-bin": "^0.109.0",
    "flow-typed": "^2.6.2",
    "gh-pages": "2.1.1",
    "husky": "^3.1.0",
    "identity-obj-proxy": "3.0.0",
